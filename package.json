{
  "name": "oquiz",
  "version": "0.0.1",
  "description": "Une application de quiz faites par les très doués Quindim",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "url": "https://github.com/O-clock-Quindim/S10"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "db:create": "psql -U oquiz -f data/create-tables.sql",
    "db:seed": "psql -U oquiz < data/seed-data.sql",
    "db:reset": "npm run db:create && npm run db:seed"
  },
  "keywords": [
    "POO",
    "oquiz",
    "quiz",
    "postgresql",
    "dotenv"
  ],
  "author": "Oclock-Quindim",
  "dependencies": {
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "overrides": [
      {
        "files": "*.js",
        "options": {
          "parser": "babel"
        }
      },
      {
        "files": "*.json",
        "options": {
          "parser": "json",
          "tabWidth": 2
        }
      }
    ],
    "semi": true,
    "requirePragma": false,
    "proseWrap": "preserve",
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "css",
    "quoteProps": "as-needed"
  }
}
